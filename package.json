{
  "name": "@enercity-ag/whisper-check",
  "version": "automatically set by release workflow",
  "description": "WhisperCheck is an command-line based utility for ThunderClient",
  "main": "src/index.js",
  "scripts": {
    "convert": "node index.js convert \"./demo-data/tc_col_fulltest.json\" \"./demo-data/collection.json\"",
    "check": "node index.js check \"./demo-data/collection.json\" --envFile \"./demo-data/environment.json\" ",
    "check:multiple": "node index.js check \"./demo-data/collection.json\" --envFile \"./demo-data/environment.json\" --multiple-choice"
  },
  "bin": {
    "whisper-check": "index.js"
  },
